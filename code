
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner soj = new Scanner(System.in);
        int n = soj.nextInt();
        int arr[] = new int[n];
        int res = 0;
        int j;
        for(int i=0;i<n;++i){
            arr[i] = soj.nextInt();
            res = res+arr[i];
        }
        for(int i=0;i<n-1;i++){
            System.out.print(res-arr[i]+" ");
            res = res - arr[i];
        }

    }
}
